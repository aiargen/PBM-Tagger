   AND `SRM:Request`.`Category 2` IS NOT NULL
   AND `SRD:MultipleQuestionResponse`.`Answer In Char` LIKE &#39;%incident%&#39;
   AND (
      `SRM:Request`.`Completion Date` BETWEEN &#39;01/01/2025 12:00:00 AM&#39; AND &#39;07/09/2025 08:59:30 AM&#39;
