      OR `SRM:Request`.`Completion Date` IS NULL
   )
)
GROUP BY 
   YEAR(`SRM:Request`.`Completion Date`),
   `SRD:ServiceRequestDefinition`.`SRD_Title`,
   `SRM:Request`.`Status`


